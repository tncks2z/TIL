    "def factorial(n):\n",
    "    result = 1\n",
    "    if n>0:\n",
    "        result = n * factorial(n-1)\n",
    "    return result\n",
    "\n",
    "n = int(input())\n",
    "print(factorial(n))\n",
    "    "
